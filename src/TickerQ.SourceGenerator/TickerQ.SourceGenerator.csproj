<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;</TargetFrameworks>
        <LangVersion>12.0</LangVersion>
        <AssemblyName>TickerQ.SourceGenerator</AssemblyName>
        <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
        <EnforceExtendedAnalyzerRules>true</EnforceExtendedAnalyzerRules>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.10.0" PrivateAssets="all" />
        <PackageReference Include="System.Memory" Version="4.5.5" PrivateAssets="all" />
        <PackageReference Include="System.Text.Json" Version="8.0.5" PrivateAssets="all" />
        <PackageReference Include="IndexRange" Version="1.0.3" PrivateAssets="all" />
    </ItemGroup>
</Project>
